@model IEnumerable<QuickySaleOnlineShopper.Models.FoodItem>

@*<ul id="album-list" >*@
@*<div id="mainbody">*@
<div id="Top">
    <ul id="TOpcategories">
        @foreach (var genre in Model)
        {

            <li>
                <a href="@Url.Action("Details", "Store",new { id = genre.FoodItemId })">
                    <img alt="@genre.AlternateText"
                         src="@Url.Content(genre.FoodPicture)" />
                    @*<span>@genre.FoodName </span>*@
                </a>

            </li>
        }
    </ul>
</div>
