@model IEnumerable<QuickySaleOnlineShopper.Models.FoodWeightType>

@{
    ViewBag.Title = "Food Packaging List";
}

<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Create New Package Type", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FoodWeightTypeName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FoodWeightTypeName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FoodWeightTypeId }) 
            @*@Html.ActionLink("Details", "Details", new { id=item.FoodWeightTypeId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.FoodWeightTypeId })*@
        </td>
    </tr>
}

</table>
<div>
    @Html.ActionLink("Back", "Index", "StoreManagerItems")
</div>